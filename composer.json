{
    "name": "netflex/database",
    "type": "library",
    "description": "Netflex Database driver for Laravel",
    "keywords": [
        "netflex",
        "laravel",
        "database"
    ],
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "autoload": {
        "psr-4": {
            "Netflex\\Database\\": "src/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "netflex/api": "^4.0",
        "netflex/dbal": "^1.0|^2.0",
        "netflex/query-builder": "^4.0"
    },
    "require-dev": {
        "illuminate/config": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netflex\\Database\\Providers\\DatabaseServiceProvider"
            ]
        }
    }
}