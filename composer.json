{
    "name": "netflex/database",
    "type": "library",
    "description": "Netflex Database driver for Laravel",
    "keywords": [
        "netflex",
        "laravel",
        "database"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "autoload": {
        "psr-4": {
            "Netflex\\Database\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "netflex/api": "^4.0",
        "netflex/dbal": "^3.0",
        "netflex/query-builder": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netflex\\Database\\Providers\\DatabaseServiceProvider"
            ]
        }
    }
}